/* 管理员列表 */

.card {
  margin: 5px 15px;
  padding: 8px;
  border-radius: 5px;
  display: flex;
  background-color: #fff;
}

.icon {
  width: 80px;
  height: 80px;
  border-radius: 5px;
}

.icon image {
  width: 100%;
  height: 100%;
  border-radius: 5px;
}

.info-box {
  margin-left: 10px;
}

.info {
  font-size: 14px;
  font-weight: bold;
}

.note {
  margin: 10rpx 0rpx 10rpx 0rpx;
  font-size: 10px;
}
.text-red{
  color: red;
}